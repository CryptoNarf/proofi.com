<!doctype html>
<html lang="en">

{% set title = title %}
{% set description = description %}
{% set currentUrl = currentUrl %}
{% set canonical = canonical %}
{% set ogTitle = ogTitle %}
{% set ogDescription = ogDescription %}
{% set ogUrl = ogUrl %}

<head>
  <meta name="robots" content="all">
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1.0, maximum-scale=1.0">

    <title>{{ title }}</title>
  <link rel="canonical" href="https://www.proofi.com{{ currentUrl }}" />
  <link rel="icon" href="/assets/favicon.png" sizes="32x32"/>
  <meta property="og:locale" content="nl_NL" />
  <meta name="type" property="og:type" content="website" />
  <meta name="title" property="og:title" content="{{ title }}" />
  <meta name="description" property="og:description" content="{{ description }}"/>
  <meta name="image" property="og:image" content="https://www.proofi.com{{ image }}" />
  <meta name="url" property="og:url" content="https://www.proofi.com{{ currentUrl }}" />
  <meta name="site_name" property="og:site_name" content="Proofi" />
  <meta name="twitter:card" content="summary" />
  <meta name="twitter:title" content="{{ title }}" />
  <meta name="twitter:description" content="{{ description }}" />
  <meta name="twitter:image" content="https://www.proofi.com{{ image }}" />

  <link rel="stylesheet" href="/css/main.css">

</head>


<body>
  {% include "partials/navigation.njk" %}

  {% block content %}{% endblock %}


  {% include "partials/footer.njk" %}

  <script>window.lazyLoadOptions = { elements_selector: ".lazy" };</script>
  <script src="/js/main.js"></script>
